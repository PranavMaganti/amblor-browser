{
  "name": "amblor",
  "version": "1.0.0",
  "description": "A music scrobbling client for amblor",
  "author": "Pranav Maganti(vanpra)",
  "license": "MIT",
  "staticFiles": {
    "staticPath": "public",
    "distDir": "dist"
  },
  "engines": {
    "node": "15.13.0"
  },
  "scripts": {
    "watch": "parcel watch src/background.ts src/options.tsx  src/popup.tsx src/connectors/*.ts",
    "build": "parcel build src/background.ts src/options.tsx src/popup.tsx src/connectors/*.ts",
    "clean": "rimraf dist",
    "test": "npx jest",
    "style": "prettier --write \"src/**/*.{ts,tsx}\""
  },
  "dependencies": {
    "axios": "^0.21.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@material-ui/core": "^4.11.3",
    "@parcel/transformer-typescript-tsc": "^2.0.0-beta.2",
    "@parcel/validator-typescript": "^2.0.0-beta.2",
    "@types/jest": "^26.0.22",
    "@types/node": "^15.0.1",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.0",
    "copy-webpack-plugin": "^10.0.0",
    "jest": "^26.6.3",
    "parcel": "^2.0.0-beta.2",
    "parcel-reporter-static-files-copy": "^1.2.1",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2 ",
    "ts-jest": "^26.5.4 ",
    "ts-loader": "^9.1.1",
    "typescript": "~3.9.9 ",
    "webextension-polyfill-ts": "^0.25.0"
  },
  "browserslist": [
    "last 3 Chrome version"
  ]
}
