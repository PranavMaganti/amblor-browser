{
  "manifest_version": 2,
  "name": "amblor",
  "description": "A music scrobbling client for amblor",
  "version": "1.0.0",
  "permissions": ["tabs", "identity", "storage", "<all_urls>"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "icons": { 
    "48": "icon48.png",
   "128": "icon128.png" 
  }
}
